{
  "name": "walli",
  "version": "0.0.5",
  "main": "lib/walli.js",
  "types": "lib/walli.d.ts",
  "description": "A functional style validation library.",
  "author": "imcuttle",
  "scripts": {
    "test": "jest",
    "dist": "rimraf lib && tsc",
    "prepublishOnly": "npm run dist",
    "site": "rimraf _site && typedoc src/walli.ts --readme Readme.md --out _site/ --theme default && touch _site/.nojekyll",
    "preversion": "npm test && npm run changelog",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "walli",
    "validation",
    "valid"
  ],
  "repository": "imcuttle/walli",
  "jest": {
    "collectCoverage": true,
    "testMatch": [
      "**/__tests__/**/*.{j,t}s?(x)",
      "**/?(*.)(spec|test).{j,t}s?(x)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "@types/lodash": "^4.14.106",
    "@types/node": "^9.6.2",
    "conventional-changelog-cli": "^1.3.21",
    "jest": "^22.4.3",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.4.2",
    "typedoc": "^0.11.1",
    "typescript": "^2.8.1"
  },
  "dependencies": {
    "json-stringify-safe": "^5.0.1",
    "lodash": "^4.17.5"
  }
}
